<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hashim.org.clevermindpobict">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/clever_mind_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_clever_mind_circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true"
        tools:targetApi="m"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity android:name=".LatestEvent" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".LatestNewsActivity" />
        <activity android:name=".AttendedActivity" />
        <activity android:name=".JobsActivity" />
        <activity android:name=".Courses" />
        <activity android:name=".Profile" />
        <activity android:name=".Payment" />
        <activity android:name=".PrivacyPolicy" />
        <activity android:name=".Settings" />
        <activity android:name=".SignUp" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>



</manifest>